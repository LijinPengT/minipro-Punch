/* pages/mycenter/done/done.wxss */
.contain{
  color: #fff;
}
image{
  position: absolute;
  height: 100%;
  width: 150%;
  left: -25%;
  top: 0;
}
.program{
  width: 80%;
  border-radius: 20rpx;
  padding: 40rpx;
  margin: 30rpx auto;
  backdrop-filter: blur(2px);
  background: rgba(0, 0, 0, 0.46);
  overflow: hidden;
}

.kind{
  float: left;

}
.count{
  text-align: right;
  font-size: 20px;
  font-weight: 900;
  float: right;
}
.count::before{
  content: '成功punch';
  font-size: 10px;
  display: block;
  width: 100%;
}
.width50{
  width: 50%;
}
inline-block{
  display: inline-block;
}